<Window x:Class="CollageApp.ImagePropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CollageApp"
        mc:Ignorable="d"
        Title="ImagePropertiesWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="All Image Properties" FontWeight="Bold" FontSize="14" />
            <ListBox x:Name="PropertiesListBox" />
        </StackPanel>
    </Grid>
</Window>
